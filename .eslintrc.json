{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": ["standard"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "semi": ["error", "always"],
      "no-throw-literal": "off",
      "camelcase": "off",
      "valid-typeof": "off",
      "no-useless-constructor": "off"
    }
  }